{
  "name": "colabclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "build:android:apk": "bash scripts/build-android-apk.sh",
    "build:android:bundle": "bash scripts/build-android-bundle.sh",
    "build:android:install": "bash scripts/install-android-apk.sh",
    "build:ios:ipa": "bash scripts/build-ios-ipa.sh",
    "wiremock:start": "bash scripts/start-wiremock.sh",
    "wiremock:start:npm": "cd wiremock && npm start",
    "wiremock:stop": "cd wiremock && npm stop",
    "wiremock:stop:docker": "cd wiremock && docker-compose down",
    "wiremock:logs": "cd wiremock && docker-compose logs -f",
    "wiremock:list": "bash scripts/list-wiremock-endpoints.sh",
    "wiremock:export": "bash scripts/export-wiremock-mappings.sh",
    "wiremock:build-push-start": "bash scripts/build-push-start-wiremock.sh",
    "wiremock:build-push-start:help": "echo 'Usage: npm run wiremock:build-push-start [version] [registry] [push] [use-registry]' && echo '  version: Image version tag (default: latest)' && echo '  registry: Docker Hub username or registry URL (optional)' && echo '  push: true to push to registry (default: false)' && echo '  use-registry: true to use registry image instead of building (default: false)'"
  },
  "dependencies": {
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "^0.80.1",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/elements": "^2.8.2",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.13.2",
    "i18next": "^23.7.16",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "react": "^19.1.0",
    "react-i18next": "^14.0.1",
    "react-native": "^0.80.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^6.3.11",
    "react-native-config": "^1.6.0",
    "react-native-crypto-js": "^1.0.0",
    "react-native-curved-bottom-bar": "^3.5.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-keychain": "^10.0.0",
    "react-native-localize": "^3.0.3",
    "react-native-maps": "^1.26.18",
    "react-native-material-ripple": "^0.9.1",
    "react-native-mmkv": "^3.3.3",
    "react-native-otp-entry": "^1.8.5",
    "react-native-permissions": "^5.4.4",
    "react-native-reanimated": "^4.1.5",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.15.0",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.6.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@react-native-community/cli": "^19.0.0",
    "@react-native-community/cli-platform-android": "^19.0.0",
    "@react-native-community/cli-platform-ios": "^19.0.0",
    "@react-native/babel-preset": "^0.80.1",
    "@react-native/eslint-config": "^0.80.1",
    "@react-native/metro-config": "^0.80.1",
    "@react-native/typescript-config": "^0.80.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.5",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "prettier": "^2.8.8",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
