{
  "request": {
    "method": "POST",
    "urlPathPattern": "/api/organization/[0-9]+/deactivate",
    "headers": {
      "Authorization": {
        "matches": "Bearer .+"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "message": "Organization deactivated successfully"
    }
  }
}

