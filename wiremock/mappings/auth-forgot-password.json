{
  "request": {
    "method": "POST",
    "url": "/api/auth/forgot-password",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "success": true,
      "message": "If the email exists, a password reset OTP has been sent to your email",
      "accountStatus": "active",
      "otpSent": true
    }
  }
}

