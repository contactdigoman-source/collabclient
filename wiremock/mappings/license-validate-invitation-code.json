{
  "request": {
    "method": "POST",
    "url": "/api/license/validate-invitation-code",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.invitationCode"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "isValid": true,
      "licenseId": 1,
      "message": "Invitation code is valid",
      "isInviteOnlyMode": false
    }
  }
}

