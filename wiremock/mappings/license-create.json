{
  "request": {
    "method": "POST",
    "url": "/api/license",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      },
      "Authorization": {
        "matches": "Bearer .+"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.licenseKey"
      },
      {
        "matchesJsonPath": "$.licenseType"
      },
      {
        "matchesJsonPath": "$.maxUsers"
      },
      {
        "matchesJsonPath": "$.maxRegions"
      }
    ]
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json",
      "Location": "/api/license/1"
    },
    "jsonBody": {
      "id": 1,
      "licenseKey": "LICENSE-2024-002",
      "licenseType": "Enterprise",
      "maxUsers": 100,
      "maxRegions": 10,
      "startDate": "2024-01-01T00:00:00Z",
      "endDate": "2024-12-31T23:59:59Z",
      "isActive": true,
      "enterpriseId": null,
      "createdAt": "2024-01-15T00:00:00Z"
    }
  }
}

