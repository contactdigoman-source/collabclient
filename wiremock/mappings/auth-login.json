{
  "request": {
    "method": "POST",
    "url": "/api/auth/login",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.email",
          "matches": ".*"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.password",
          "matches": ".*"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "accountStatus": "active",
      "idpjourneyToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZHBqb3VybmV5IjoiMTIzNDU2Nzg5MCIsImlhdCI6MTUxNjIzOTAyMn0.abc123def456ghi789jkl012mno345pqr678stu901vwx234yz",
      "message": "Login successful. Please verify OTP to continue."
    }
  }
}
