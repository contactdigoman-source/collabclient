{
  "request": {
    "method": "POST",
    "url": "/api/administrators/news/upload-document",
    "headers": {
      "Content-Type": {
        "matches": "multipart/form-data.*"
      }
    },
    "multipartPatterns": [
      {
        "matchingType": "ANY",
        "headers": {
          "Content-Disposition": {
            "matches": ".*name=\"document\".*"
          }
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "success": true,
      "message": "News document uploaded successfully",
      "documentUrl": "https://example.com/documents/news-1.pdf",
      "documentName": "document.pdf"
    }
  }
}

