{
  "request": {
    "method": "POST",
    "urlPathPattern": "/api/invite/[0-9]+/resend",
    "headers": {
      "Authorization": {
        "matches": "Bearer .+"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "message": "Invitation resent successfully"
    }
  }
}

