{
  "request": {
    "method": "POST",
    "url": "/api/auth/reset-password",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.token"
      },
      {
        "matchesJsonPath": "$.newPassword"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "message": "Password reset successfully"
    }
  }
}

