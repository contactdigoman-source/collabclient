{
  "name": "nexval-wiremock",
  "version": "1.0.0",
  "description": "WireMock stubs for Nexval Attendance API",
  "scripts": {
    "start": "wiremock --port 8080 --root-dir ./mappings --global-response-templating --verbose --bind-address 0.0.0.0",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down",
    "logs:docker": "docker-compose logs -f"
  },
  "keywords": [
    "wiremock",
    "api",
    "stubs",
    "mock"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "wiremock": "^2.35.0"
  }
}

